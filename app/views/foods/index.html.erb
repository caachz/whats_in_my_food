<h1><%= @foods.count %> Results</h1>
<% @foods.first(10).each do |food| %>
  <ul class="food">
    <li class="upccode"><%= food.upc_code %></li>
    <li class="description"><%= food.description %></li>
    <li class="BrandOwner"><%= food.brand_owner %></li>
    <li class="ingredients"><%= food.ingredients %></li>
  </ul>
<% end %>
